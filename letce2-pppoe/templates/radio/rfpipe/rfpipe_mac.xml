<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mac SYSTEM "file:///usr/share/emane/dtd/mac.dtd">
<mac name="RF-PIPE MAC" library="rfpipemaclayer">
  <param name="enablepromiscuousmode"         value="off"/>
  <param name="pcrcurveuri"                   value="rfpipe_pcr.xml"/>
  <param name="datarate"                      value="1M"/>
  <param name="delay"                         value="0.0"/>
  <param name="jitter"                        value="0.0"/>
  <param name="flowcontrolenable"             value="off"/>
  <param name="radiometricenable"             value="on"/>
  <param name="radiometricreportinterval"     value="10"/>
  <param name="neighbormetricdeletetime"      value="20"/>
</mac>
